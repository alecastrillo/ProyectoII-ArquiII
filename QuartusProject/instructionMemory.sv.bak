module instructionMemory ( input logic [31:0] A,
								   output logic [31:0] RD
									);
	// Tama√±o de memoria de instrucciones 32x32
	logic [31:0] memory [0:31];
	initial
		begin 
			$readmemh("instructionsMemory.dat",memory);
		end
	
	// La instruccion esta desfazada 8 bytes asi que se le elimininan los dos iniciales
	assign RD = memory[A[31:2]];

endmodule 